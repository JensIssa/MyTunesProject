<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jens.GUI.AddPlaylistController">
   <children>
      <TextField fx:id="playlistName" layoutX="94.0" layoutY="38.0" prefHeight="26.0" prefWidth="247.0" />
      <Label layoutX="41.0" layoutY="42.0" text="Name:" />
      <ButtonBar layoutX="141.0" layoutY="83.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelNewEdit" text="Cancel" />
            <Button fx:id="savePlaylist" mnemonicParsing="false" onAction="#addPlaylist" text="Save" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
