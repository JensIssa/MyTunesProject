<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jens.GUI.MainWindowController">
   <children>
      <TableView fx:id="playlistTable" layoutX="14.0" layoutY="41.0" onMouseClicked="#lookAtPlaylist" prefHeight="306.0" prefWidth="228.0">
        <columns>
          <TableColumn fx:id="playlistNameColumn" prefWidth="103.0" text="Name" />
          <TableColumn fx:id="playlistSongsColumn" prefWidth="47.0" text="Songs" />
            <TableColumn fx:id="playlistTimeColumn" prefWidth="78.0" text="Time" />
        </columns>
      </TableView>
      <ButtonBar buttonMinWidth="50.0" layoutX="70.0" layoutY="10.0" prefHeight="25.0" prefWidth="173.0">
        <buttons>
            <Button fx:id="playlistNew" minWidth="0.0" mnemonicParsing="false" onAction="#newPlaylist" prefHeight="25.0" text="New" textAlignment="CENTER" />
          <Button fx:id="playlistEdit" minWidth="60.0" mnemonicParsing="false" onAction="#editPlaylist" prefHeight="25.0" prefWidth="86.0" text="Edit" textAlignment="CENTER" />
            <Button fx:id="playlistDelete" minWidth="62.0" mnemonicParsing="false" onAction="#deletePlaylist" prefHeight="25.0" text="Delete" textAlignment="CENTER" />
        </buttons>
         <rotationAxis>
            <Point3D />
         </rotationAxis>
         <padding>
            <Insets left="-20.0" />
         </padding>
      </ButtonBar>
      <ListView fx:id="songsInPlaylistListView" layoutX="250.0" layoutY="42.0" prefHeight="484.0" prefWidth="200.0" />
      <TableView fx:id="songTable" layoutX="505.0" layoutY="41.0" prefHeight="484.0" prefWidth="381.0">
        <columns>
          <TableColumn fx:id="songTitleColumn" prefWidth="123.0" text="Title" />
          <TableColumn fx:id="songArtistColumn" prefWidth="118.0" text="Artist" />
            <TableColumn fx:id="songCategoryColumn" prefWidth="83.0" text="Category" />
            <TableColumn fx:id="songTimeColumn" prefWidth="56.0" text="Time" />
        </columns>
      </TableView>
      <ButtonBar layoutX="641.0" layoutY="520.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="songNew" mnemonicParsing="false" onAction="#newSong" text="New" />
            <Button fx:id="songEdit" mnemonicParsing="false" onAction="#editSong" text="Edit" />
            <Button fx:id="songDelete" mnemonicParsing="false" onAction="#deleteSong" text="Delete" />
        </buttons>
         <padding>
            <Insets left="-20.0" />
         </padding>
      </ButtonBar>
      <ButtonBar layoutX="802.0" layoutY="529.0" prefHeight="40.0" prefWidth="84.0" />
      <TextField fx:id="searchTextField" layoutX="687.0" layoutY="11.0" prefHeight="25.0" prefWidth="200.0" />
      <Label layoutX="14.0" layoutY="14.0" text="Playlists" />
      <Label layoutX="249.0" layoutY="23.0" text="Songs on Playlist" />
      <Label layoutX="505.0" layoutY="14.0" text="Songs" />
      <Label layoutX="628.0" layoutY="14.0" text="Search:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="directAddSongToPlaylist" layoutX="456.0" layoutY="130.0" mnemonicParsing="false" onAction="#addSongToPlaylist" prefHeight="40.0" prefWidth="40.0">
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/arrowLeft.png" />
               </image></ImageView>
         </graphic>
      </Button>
      <Button fx:id="moveDown" layoutX="456.0" layoutY="89.0" mnemonicParsing="false" onAction="#moveSongDown">
         <graphic>
            <ImageView fx:id="downButton" fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/arrowDown.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
    <Button fx:id="moveUp" layoutX="456.0" layoutY="42.0" mnemonicParsing="false" onAction="#moveSongUp">
         <graphic>
            <ImageView fx:id="upButton" fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/arrowUp.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="playlistSongDelete" layoutX="250.0" layoutY="528.0" mnemonicParsing="false" onAction="#deletePlaylistSong" text="Delete" />
      <Slider fx:id="volumeSlider" layoutX="755.0" layoutY="571.0" max="110.0" min="-1.0" onMouseDragged="#adjustVolume" prefHeight="14.0" prefWidth="131.0" value="110.0" />
      <Button fx:id="playButton" layoutX="460.0" layoutY="531.0" mnemonicParsing="false" onAction="#playSong" onMouseClicked="#setSongImage" prefHeight="36.0" prefWidth="28.0" style="-fx-background-radius: 300;">
         <graphic>
            <ImageView fitHeight="31.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fitHeight="20.0" fitWidth="21.0" layoutX="735.0" layoutY="568.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Icons/volume.png" />
         </image>
      </ImageView>
      <Button fx:id="forwardButton" layoutX="504.0" layoutY="531.0" mnemonicParsing="false" onAction="#nextSong" prefHeight="36.0" prefWidth="36.0" style="-fx-background-radius: 200;">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/forward.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="rewindButton" layoutX="414.0" layoutY="531.0" mnemonicParsing="false" onAction="#previousSong" prefHeight="36.0" prefWidth="21.0" style="-fx-background-radius: 200;">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Icons/rewind.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fx:id="songImage" fitHeight="222.0" fitWidth="218.0" layoutX="19.0" layoutY="347.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../image/unnamed.png" />
         </image></ImageView>
      <Label fx:id="labelIsPlaying" alignment="CENTER" layoutX="19.0" layoutY="578.0" prefHeight="17.0" prefWidth="218.0" text="(Nothing) Is Playing">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutX="246.0" layoutY="569.0" prefHeight="18.0" prefWidth="465.0" progress="0.0" />
   </children>
</AnchorPane>
